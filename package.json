{
  "name": "esy-flex",
  "version": "2.6.4000",
  "description": "Flex packaged for esy",
  "esy": {
    "exportedEnv": {
      "LD_LIBRARY_PATH": {
        "val": "#{self.lib}:$LD_LIBRARY_PATH",
        "scope": "global"
      }
    },
    "buildsInSource": true,
    "build": [
      "./configure --prefix=#{self.install}",
      "make"
    ],
    "install": [
      "make install"
    ]
  }
}
